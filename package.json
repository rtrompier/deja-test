{
  "name": "@deja-js/component",
  "version": "2.0.0",
  "description": "Angular2 Components Library",
  "homepage": "https://github.com/DSI-HUG/dejajs-components",
  "bugs": "https://github.com/DSI-HUG/dejajs-components/issues",
  "author": "Team DSI-HUG - Hôpital Univesitaire de Genève - DSI - Domaine Patient",
  "license": "Apache-2.0",
  "keywords": [
    "angular",
    "material",
    "component",
    "angular-cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DSI-HUG/dejajs-components.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --env=prod --base-href https://dsi-hug.github.io/dejajs-components/",
    "build:lib": "gulp build",
    "build:lib:watch": "gulp",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n @deja-js/component -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "licensecheck": "license-check",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin"
  },
  "dependencies": {
    "@angular/animations": "^4.1.3",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.5",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@deja-js/resources": "^1.1.4",
    "angular2-text-mask": "^8.0.1",
    "codelyzer": "^2.0.1",
    "core-js": "^2.4.1",
    "moment": "2.18.1",
    "prismjs": "1.6.0",
    "rxjs": "5.4.0",
    "showdown": "1.6.4",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "4.1.3",
    "@compodoc/compodoc": "^1.0.0-beta.7",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "@types/prismjs": "1.6.2",
    "codelyzer": "~2.0.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.11.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.2",
    "node-sass-tilde-importer": "^1.0.0",
    "node-watch": "^0.5.2",
    "protractor": "~5.1.0",
    "rollup": "^0.41.6",
    "run-sequence": "^1.2.2",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.3.2"
  }
}
